{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 9002",
    "dev:stable": "./dev-stable.sh",
    "dev:persistent": "./start-dev.sh",
    "dev:3000": "next dev -p 3000",
    "dev:3001": "next dev -p 3001",
    "dev:clean": "rm -rf .next && next dev --turbopack -p 9002",
    "dev:status": "./dev-status.sh",
    "start-dev": "./start-dev.sh",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "NODE_ENV=production next build",
    "build:fast": "next build --experimental-build-mode=compile",
    "export": "NODE_ENV=production next build",
    "deploy:build": "npm run export",
    "deploy:firebase": "firebase deploy --only hosting",
    "deploy": "npm run deploy:build && npm run deploy:firebase",
    "start": "next start",
    "lint": "next lint --fix",
    "lint:check": "next lint",
    "typecheck": "tsc --noEmit --incremental",
    "typecheck:watch": "tsc --noEmit --incremental --watch",
    "test": "NODE_ENV=test jest --passWithNoTests",
    "test:watch": "NODE_ENV=test jest --watch --passWithNoTests",
    "test:coverage": "NODE_ENV=test jest --coverage --passWithNoTests",
    "test:ci": "NODE_ENV=test jest --ci --coverage --passWithNoTests --maxWorkers=2",
    "test:debug": "NODE_ENV=test node --inspect-brk node_modules/.bin/jest --runInBand",
    "verify": "npm run typecheck && npm run test:ci",
    "clean": "rm -rf .next .swc node_modules/.cache",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
